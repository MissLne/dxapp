/* pages/activityPage/manage/memberMessage/memberMessage.wxss */
.menberMessBox {
  width: 654rpx;
  margin: 26rpx auto 0 auto;
  padding: 0 20rpx 12rpx 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.menberMessBox view:nth-child(1) {
  font-size: 30rpx;
  color: #333;
}
.menberMessBox view:nth-child(2) {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
  width: 300rpx;
  display: flex;
  justify-content: flex-start;
}
.menberMessBox image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 8rpx;
}