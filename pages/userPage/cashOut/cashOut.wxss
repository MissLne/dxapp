/* pages/userPage/cashOut/cashOut.wxss */
.wayChoices {
  width: 750rpx;
  height: 127rpx;
  box-sizing: border-box;
  padding: 0 54rpx;
  border-bottom: 10rpx solid #f5f5f5;
  border-top: 10rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-weight: bold;
}
.cashTips {
  width: 638rpx;
  height: 100rpx;
  margin: 190rpx auto 0 auto;
  border-radius: 30rpx;
  background: #f4f4f4;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #333;
}
.cashTips view {
  width: 534rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cashTips view text:nth-child(2) {
  color: #FF441A;
}