/* pages/loginPage/register/register.wxss */
.titleContent {
  width: 570rpx;
  /* height: 118rpx; */
  color: #666;
  font-size: 26rpx;
  text-align: center;
  margin: 100rpx auto;
  font-family: 'Ping Fang';
}
.registerInput {
  width: 580rpx;
  margin: 27rpx auto 0 auto;
  padding: 15rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(0, 0, 0,0.1);
  display: flex;
  justify-content: space-between;
}
.registerInput view {
  font-size: 26rpx;
  color: #333;
  font-family: 'Ping Fang';
}
.registerInput input {
  width: 300rpx;
  font-size: 24rpx;
  color: #999;
  font-family: 'Ping Fang';
}
.redContent {
  font-size: 24rpx;
  text-align: center;
  margin: 100rpx auto 30rpx auto;
  color: #FF441A;
}