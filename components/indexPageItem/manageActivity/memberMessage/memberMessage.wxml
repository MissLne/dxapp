<!-- components/indexPageItem/manageActivity/memberMessage/memberMessage.wxml -->
<view wx:for="{{member}}" wx:key="{{index}}">
  <view bind:tap="showDetail" data-mId="{{item.mid}}">
    <view>{{item.name}}</view>
    <view>
      <view>{{item.gender}}</view>
      <view>{{item.school}}</view>
      <view>{{item.phone}}</view>
    </view>
  </view>
  <view>{{item.ticketName}}</view>
</view>