/* components/indexPageItem/publishIntroduce/tips/tips.wxss */
.publishTitle {
  width: 690rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.publishContainer {
  width: 690rpx;
  height: 370rpx;
  font-size: 26rpx;
  margin: 0 auto;
  padding: 30rpx;
  background: #fff;
  box-sizing: border-box;
  border-radius: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.titleText {
  font-size: 18rpx;
  color: #999;
}
.uploadContainer {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.publishImg {
  width: 78rpx;
  height: 78rpx;
  margin-right: 10rpx;
  border-radius: 8rpx;
  background: #f5f5f5;
}
.publishUpload {
  color: #666;
  font-size: 22rpx;
}
.imgContainer {
  display: flex;
  justify-content: space-between;
  align-items: center;
}