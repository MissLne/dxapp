<!--components/indexPageItem/setUpPage/setUpBox/setUpBox.wxml-->
<view wx:for="{{defaultArray}}" wx:key="index">
  <view class="setUpContainer">
    <view class="containerTitle">{{item.title}}</view>
    <view class="setUpBox">
      <view class="setUpItemBox">
        <view>姓名</view>
        <input value="{{item.name}}" />
      </view>
      <view class="setUpItemBox">
        <view>性别</view>
        <radioBtn radioContent="{{['男','女']}}" widthCount="{{60}}" radio-parent-class="tworadio-parent-class" currentSelect="{{0}}"/>
      </view>
      <view class="setUpItemBox">
        <view>联系方式</view>
        <input value="{{item.phone}}" />
      </view>
      <view class="setUpItemBox">
        <view>就读学校</view>
        <input value="{{item.school}}" />
      </view>
    </view>
  </view>
</view>
