/* components/tools/comNav/comNav.wxss */
.topBarTotalHeight {
  width: 100vw;
  position: fixed;
  top: 0;
  left: 0;
  display: flex;
  align-items: flex-end;
  background: #fff;
  z-index: 9999;
}
.topBarTextHeight {
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #333;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.topNavArrow {
  width: 28rpx;
  height: 28rpx;
  padding-right: 30rpx;
}