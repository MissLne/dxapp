/* components/tools/select/select.wxss */
.selectMainBox {
  height: 30rpx;
}
.com-sContent{
  background: white;
  position: relative;
  height: 11.5pt;
}
.com-sImg{
  position: absolute;
  right: 10px;
  top: 11px;
  width: 16px;
  height: 9px;
  transition: all .3s ease;
}
.com-sList{
  background: #fff;
  width: inherit;
  position: absolute;
  top: 50rpx;
  left: 0;
  border-top: none;
  box-sizing: border-box;
  z-index: 3;
  max-height: 120px;
}
.com-sItem{
  text-align: center;
  white-space: nowrap;
}