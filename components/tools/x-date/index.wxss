.showDateB {
  width: 262rpx;
  font-size: 20rpx;
  /* color: #999; */
}
.mask{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0px;
  top: 0px;
  background: rgba(100, 100, 100, 0.6);
  z-index: 99999999;
}
.content{
  position: absolute;
  bottom: 0px;
  left: 0px;
  display: flex;
  flex-direction: column;
  height: 220px;
  width: 100%;
  background-color: white;
}
.header{
  display: flex;
  width: 100%;
  height: 40px;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0px 20px;
  font-size: 14px;
}
.select{
  height: 50px;
  color: black;
}
.cancle{
  color: #8a8a8a;
}
.confirm{
  color: #76EEC6;
}
.pick-view{
  height: 180px;
  width: 100%;
  
}
.item{
  line-height: 50px;
  text-align: center;
}
