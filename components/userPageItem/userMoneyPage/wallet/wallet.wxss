/* components/userPageItem/userMoneyPage/wallet/wallet.wxss */
.walletBox {
  margin-top: 222rpx;
}
.container {
  width: 610rpx;
  height: 100rpx;
  border-radius: 30rpx;
  padding: 0 66rpx;
  margin: 0 auto;
  box-sizing: border-box;
  background: #f5f5f5;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.container view:nth-child(1) {
  font-size: 36rpx;
  color: #333;
  margin-right: 66rpx;
}
.container view:nth-child(2) {
  font-size: 46rpx;
  color: #333;
}
.withdraw {
  color: #333;
  font-size: 26rpx;
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
}